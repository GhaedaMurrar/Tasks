def func_(n):
    s = 0
    test =[]
    while s <= n:
        test.append(s)
        s+=0.5
    return test 


def main():
    num = input("Enter a number : ")
    for n in func_(num):
        print (n)
    
    
if __name__ == "__main__":
    main()

         
        
